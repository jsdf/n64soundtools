
envelope defaultenv
{
    attackTime    = 0;
    attackVolume  = 127;
    decayTime   = 500000;
    decayVolume   = 100;
    releaseTime   = 200000;
    // this is a comment
}

sound flugelhorn {
    use ("../sounds/Ride.22k.aiff");
    
    pan    = 64;
    volume = 127;
    keymap = RideCym2Key;
    envelope = percEnv;
}


instrument Standard_Kit
{
    volume = 127;
    pan    = 64;

    sound = KickDrum2;
    sound = KickDrum1;
    sound = SideStick;
    sound = AcSnare;
    sound = HandClap;
}

keymap HighCongaKey
{
    velocityMin = 0;
    velocityMax = 127;
    keyMin      = 63;
    keyMax      = 63;
    keyBase     = 75;
    detune      = 0;
}


bank GenMidiBank
{
    sampleRate    = 44100;
    percussionDefault   = Standard_Kit;

    instrument [0] = Piano44K; 
    instrument [1] = GrandPiano;
    instrument [4] = ElecGrand;
}